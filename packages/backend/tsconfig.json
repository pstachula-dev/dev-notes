{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "baseUrl": ".",
    "moduleResolution": "node",
    "strict": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "include": ["src/global.d.ts", "src", "**/*.ts", "**/*.tsx", "api"],
  "exclude": ["node_modules"]
}
